		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"minimist": "^1.2.8",
		"sharp": "^0.34.3",
		"svelte-gestures": "^5.1.4"
	}
